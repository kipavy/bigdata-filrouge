# ================================
# Configuration PostgreSQL
# ================================
POSTGRES_USER=airflow
POSTGRES_PASSWORD=CHANGE_ME_IN_PRODUCTION
POSTGRES_DB=airflow

# ================================
# Configuration MongoDB
# ================================
MONGO_INITDB_ROOT_USERNAME=mongo
MONGO_INITDB_ROOT_PASSWORD=CHANGE_ME_IN_PRODUCTION
MONGO_DB=velib_datalake
MONGO_HOST=mongodb
MONGO_PORT=27017

# ================================
# Configuration Airflow
# ================================
AIRFLOW_IMAGE_NAME=apache/airflow:2.10.0
AIRFLOW__CORE__EXECUTOR=CeleryExecutor
AIRFLOW__DATABASE__SQL_ALCHEMY_CONN=postgresql+psycopg2://airflow:CHANGE_ME_IN_PRODUCTION@postgres/airflow
# Generate a new Fernet key for production: python -c "from cryptography.fernet import Fernet; print(Fernet.generate_key().decode())"
AIRFLOW__CORE__FERNET_KEY=GENERATE_NEW_KEY_FOR_PRODUCTION
AIRFLOW__CORE__LOAD_EXAMPLES=false
AIRFLOW_UID=50000

# ================================
# API Velib
# ================================
VELIB_API_BASE_URL=https://velib-metropole-opendata.smoove.pro/opendata/Velib_Metropole
